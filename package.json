{
   "name": "@trailimage/flickr-provider",
   "version": "3.0.0",
   "description": "Trail Image Flickr provider",
   "private": false,
   "engines": {
      "node": ">=12.0.0"
   },
   "author": {
      "name": "Jason Abbott",
      "url": "http://github.com/trailimage"
   },
   "license": "MIT",
   "publishConfig": {
      "access": "public"
   },
   "main": "cjs/index.js",
   "module": "esm/index.js",
   "types": "esm/index.d.ts",
   "files": [
      "esm",
      "cjs"
   ],
   "scripts": {
      "build:esm": "./node_modules/.bin/tsc --project ./node_modules/@toba/develop/tsconfig.esm.json",
      "build:cjs": "./node_modules/.bin/tsc --project ./node_modules/@toba/develop/tsconfig.cjs-min.json",
      "prepublishOnly": "npm run build:esm & npm run build:cjs",
      "test": "jest --coverage"
   },
   "devDependencies": {
      "@toba/develop": "^6.6.12",
      "@toba/test": "^4.1.0",
      "@types/node-fetch": "^2.1.2",
      "typescript": "^3.6.2"
   },
   "repository": {
      "type": "git",
      "url": "https://github.com/trailimage/flickr-provider"
   },
   "bugs": {
      "url": "https://github.com/trailimage/flickr-provider/issues"
   },
   "prettier": {
      "singleQuote": true,
      "tabWidth": 3
   },
   "dependencies": {
      "@toba/flickr": "^3.0.0",
      "@toba/map": "^1.3.0",
      "@toba/tools": "^6.0.0",
      "@trailimage/models": "^3.0.6"
   }
}
